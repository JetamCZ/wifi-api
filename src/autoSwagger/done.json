            "custom": {
             "type": "object",
             "properties": {
              "fingerPrint": {
               "type": "string",
               "example": "601824743480fc1dbe5de48c"
              }
             }
            },
            "rooms": {
             "type": "array",
             "items": {
              "type": "object",
              "properties": {
               "_id": {
                "type": "string",
                "example": "600ccd7b68a68a5798f6e8db"
               },
               "name": {
                "type": "string",
                "example": "Pokoj 2"
               },
               "f": {
                "type": "number",
                "example": 1
               },
               "color": {
                "type": "string",
                "example": "#886C25"
               }
              }
             }
            },
            "name": {
             "type": "string",
             "example": "Alenka"
            }
           }
          }
         },
         "customLocalizationData": {
          "type": "object",
          "properties": {
           "caclulatingTimes": {
            "type": "object",
            "properties": {
             "total": {
              "type": "number",
              "example": 244
             },
             "date": {
              "type": "string",
              "example": "2021-02-03T17:32:50.403Z"
             }
            }
           }
          }
         },
         "plan": {
          "type": "object",
          "properties": {
           "_id": {
            "type": "string",
            "example": "5ff9da5f236b1f7080d2fdeb"

{
 "/ping": {
  "get": {
   "tags": [
    "Data entry"
   ],
   "responses": {
    "200": {
     "content": {
      "application/json": {
       "schema": {
        "type": "object",
        "properties": {
         "time": {
          "type": "number",
          "example": 1612373572574
         }
        }
       }
      }
     }
    }
   }
  }
 },
 "/localization/{id}": {

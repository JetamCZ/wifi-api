          "type": "string",
          "example": "Demo"
         },
         "type": {
          "type": "string",
          "example": "NEAREST_FINGERPRINT"
         },
         "beacons": {
          "type": "array",
          "items": {
           "type": "object",
           "properties": {
            "_id": {
             "type": "string",
             "example": "5ffcc2946270ec2da06d653f"
            },
            "deviceKey": {
             "type": "string",
             "example": "00000000c05f6f00"
            },
            "x": {
             "type": "number",
             "example": 716
            },
            "y": {
             "type": "number",
             "example": 60
            },
            "f": {
             "type": "number",
             "example": 1
            },
            "name": {
             "type": "string",
             "example": "Pokoj"
            },
            "desc": {
             "type": "string",
             "example": ""
            },
            "lastSeenDate": {
             "type": "string",
             "example": "2021-02-01T22:57:41.539Z"
            }
           }
          }
         },
         "organizationId": {
          "type": "string",
          "example": "5ff9c17a236b1f7080d2fde0"
         },
         "devices": {
          "type": "array",
          "items": {
           "type": "object",
           "properties": {
            "mac": {
             "type": "string",
             "example": "3c:dc:bc:97:f2:29"
            },
            "x": {
             "type": "number",
             "example": 412.2102529006779
            },
            "y": {
             "type": "number",
             "example": 141.34375
            },
            "f": {
             "type": "number",
             "example": 1
